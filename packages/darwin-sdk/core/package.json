{
  "name": "@darwin-sdk/core",
  "version": "0.0.1",
  "description": "Core browser agent functionality for Darwin SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18.0.0 <21.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node start-server.js",
    "start:dev": "ts-node --require dotenv/config start-server.ts",
    "test:claude": "ts-node --require dotenv/config test-claude.ts",
    "test:claude:simple": "ts-node --require dotenv/config test-claude-simple.ts"
  },
  "dependencies": {
    "@browserbasehq/stagehand": "^3.0.0",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.22.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  }
}
